{
  "name": "neal-codes-app",
  "version": "0.0.1",
  "description": "Personal website for https://neal.codes",
  "main": "index.js",
  "scripts": {
    "createBuildVersion": "./scripts/buildVersion.sh",
    "copyFavicon": "./scripts/copyFavicon.sh",
    "prebuild": "npm install && npm run createBuildVersion && npm run copyFavicon",
    "build": "webpack --config ./webpack/webpack.all.config.babel.js --progress",
    "prestart": "npm run build",
    "start": "NODE_ENV=production forever ./server/build/production.server.bundle.js",
    "predevelop": "npm install && npm run createBuildVersion",
    "develop": "concurrently --kill-others \"npm run develop:build:server\" \"npm run develop:exec:server\" \"npm run develop:exec:server:watch\"",
    "develop:build:server": "webpack --config ./webpack/webpack.server.development.config.babel.js -d --watch",
    "develop:exec:server": "NODE_ENV=development NODE_TLS_REJECT_UNAUTHORIZED=0 nodemon --watch ./server/build --exec babel-node ./server/build/development.server.bundle.js",
    "develop:exec:server:watch": "NODE_ENV=development babel-node ./server/development.server.watch.js",
    "test": "NODE_ENV=test mocha-webpack",
    "test:watch": "npm run test -- --watch",
    "test:cover": "NODE_ENV=test nyc mocha-webpack",
    "bundle:analyze": "webpack --config ./webpack/webpack.bundle.analyzer.config.babel.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nealfennimore/neal.codes-app.git"
  },
  "author": "Neal Fennimore",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nealfennimore/neal.codes-app/issues"
  },
  "homepage": "https://github.com/nealfennimore/neal.codes-app#readme",
  "browserslist": [
    "last 2 versions"
  ],
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-plugin-transform-regenerator": "https://registry.npmjs.org/babel-plugin-transform-regenerator/-/babel-plugin-transform-regenerator-6.14.0.tgz",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.24.0",
    "babel-runtime": "^6.23.0",
    "chai": "^3.5.0",
    "cheerio": "^0.22.0",
    "concurrently": "^3.1.0",
    "css-loader": "^0.28.1",
    "d3-queue": "^3.0.3",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-nfen": "0.0.16",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "istanbul": "^0.4.5",
    "jimp": "^0.2.27",
    "jsdom": "^9.12.0",
    "jsdom-global": "^2.1.1",
    "json-loader": "^0.5.4",
    "loader-utils": "^0.2.17",
    "mocha": "^3.2.0",
    "mocha-webpack": "^1.0.0-beta.1",
    "modularscale-sass": "^2.1.1",
    "node-sass": "^4.5.3",
    "nyc": "^10.2.0",
    "path": "^0.12.7",
    "postcss": "^5.0.21",
