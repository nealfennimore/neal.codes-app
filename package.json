    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.25.0",
    "eslint-config-nfen": "0.0.7",
    "eslint-plugin-react": "^6.2.0",
