    "react-helmet": "^5.0.2",
    "react-redux": "^4.4.5",
    "react-router": "https://registry.npmjs.org/react-router/-/react-router-3.0.0.tgz",
    "react-router-redux": "https://registry.npmjs.org/react-router-redux/-/react-router-redux-4.0.7.tgz",
    "redux": "^3.6.0",
    "redux-saga": "^0.15.3",
    "serialize-javascript": "^1.3.0",
    "superagent": "^3.0.0",
    "webpack": "^2.5.1"
  }
}
