{
    "presets": [
        "es2015",
        "react",
        "stage-3"
    ],
    "plugins": [
        "transform-object-rest-spread",
        "transform-regenerator",
        "syntax-dynamic-import"
    ],
    "env": {
        "development": {
            "plugins": ["react-hot-loader/babel"]
        },
        "test": {
            "plugins": ["istanbul"]
        }
    }
}
